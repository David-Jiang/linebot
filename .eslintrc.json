{
    "parser": "babel-eslint",
    "extends": "airbnb-base",
    "rules": {
      "semi": ["error", "never"],
      "linebreak-style": 0,
      "no-tabs": 0,
      "indent": 0,
      "no-unused-expressions": 0,
      "prefer-const": 0,
      "no-trailing-spaces": 0,
      "max-len": 0,
      "no-plusplus": 0,
      "no-extra-boolean-cast": 0,
      "no-console": 0
    },
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    },
    "env": {
      "browser": true,
      "node": true
    },
    "plugins": [
      "import"
    ]
  }